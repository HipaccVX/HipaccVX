[NAME]
Saturate

[KERNEL_VARIABLES]
Accessor input

[KERNEL]
int out = input();
if(@@@U8@@@)
{
	if (out > 255) // max u8
		out = 255;
	else if (out < 255) // min u8
		out = 0;
}
else if (@@@S16@@@)
{
	if (out > 32767) // max s16
		out = 32767;
	else if (out < -32768) // min s16
		out = -32768;
}
output() = out;
[KERNEL_END]
